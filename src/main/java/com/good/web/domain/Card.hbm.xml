<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.good.web.domain.Card" table="tb_card">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="title" column="title" type="java.lang.String" />
        <property name="img" column="img" type="java.lang.String" />
        <property name="url" column="url" type="java.lang.String" />
        <property name="cardType" column="card_type" type="java.lang.Integer" />
        <property name="createTime" column="create_time" type="java.sql.Timestamp" />
        <property name="sorting" column="sorting" type="java.lang.Integer" />
        <property name="state" column="state" type="java.lang.Integer" />
    </class>
</hibernate-mapping>
